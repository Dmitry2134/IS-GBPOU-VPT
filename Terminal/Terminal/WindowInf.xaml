<Window x:Class="Terminal.WindowInf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Terminal"
        mc:Ignorable="d"
        Title="WindowInf" Height="450" Width="800" WindowState="Maximized" ResizeMode="NoResize" WindowStyle="None" Background="#273340">
    <Grid>
        <Button Content="" BorderBrush="#273340" Height="74" HorizontalAlignment="Right" Width="86" VerticalAlignment="Top" Click="Button_Click">
            <Button.Background>
                <ImageBrush ImageSource="back.png" Stretch="UniformToFill"/>
            </Button.Background>
        </Button>
        <Image HorizontalAlignment="Left" Height="45" VerticalAlignment="Top" Width="104" Source="logo.png">
            <Image.OpacityMask>
                <ImageBrush ImageSource="logo.png"/>
            </Image.OpacityMask>
        </Image>

        <StackPanel x:Name="panelKorpus" Margin="0,80, 0, 0" Orientation="Horizontal" VerticalAlignment="Top">
        </StackPanel>

    </Grid>
</Window>
